<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="NHibernateTest.Data.Vo"
                   assembly="NHibernateTest">
  <class name="Daysheet" table="dbo.Daysheet">
    <id name="Id" column="Id">
      <generator class="identity"/>
    </id>

    <property name="Date" column="Date"/>
    <property name="WorkedHour" column="WorkedHour"/>
    <many-to-one name="Timesheet"
		    unique="true"
        class="Timesheet"
		    column="TimesheetId"
        not-null="true"/>
  </class>

</hibernate-mapping>
