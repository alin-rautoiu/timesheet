<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="NHibernateTest.Data.Vo"
                   assembly="NHibernateTest">
  <class name="Task" table="Dbo.TaskName">
    <id name="Id" column="Id">
      <generator class="identity"/>
    </id>

    <property name="TaskName" column="TaskName"/>
    <many-to-one name="ProjectId"
        column="ProjectId" not-null="false"/>
  </class>
</hibernate-mapping>
